{
  "name": "nord-native-theme",
  "displayName": "Nord Native",
  "version": "0.0.1",
  "publisher": "divanvisagie",
  "description": "The Arctic Ice Studio Nord theme with the blueish tint removed to match the dark mode background colours of the native os.",
  "contributes": {
    "themes": [
      {
        "label": "Nord Native",
        "uiTheme": "vs-dark",
        "path": "./themes/nord-color-theme.json"
      }
    ]
  },
  "engines": {
    "vscode": "^1.12.0"
  },
  "scripts": {
    "docs": "http-server docs",
    "build": "vsce package --out nord-native-visual-studio-code.vsix"
  },
  "categories": [
    "Themes"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/divanvisagie/nord-native.git"
  },
  "keywords": [
    "vscode-theme"
  ],
  "author": "Divan Visagie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/divanvisagie/nord-native/issues"
  },
  "homepage": "https://dvisagie.com/nord-native",
  "devDependencies": {
    "http-server": "^0.12.3",
    "vsce": "^1.77.0"
  }
}
